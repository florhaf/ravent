<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/eventbackends?options=fetch</url>
    <description>Fetch and cache events</description>
    <schedule>every day 06:15</schedule>
    <timezone>America/New_York</timezone>
    <target>eventbackend</target>
  </cron>
  <cron>
    <url>/eventbackends?options=delete</url>
    <description>Delete past events</description>
    <schedule>every 1 hours synchronized</schedule>
    <timezone>America/New_York</timezone>
    <target>eventdelete</target>
  </cron>
  <cron>
    <url>/eventbackends?options=notify_access_exp</url>
    <description>Notify users when access token expired</description>
    <schedule>every friday 08:00</schedule>
    <timezone>America/New_York</timezone>
    <target>eventnotif</target>
  </cron>
</cronentries>
